for file in \
  $(find gcc/config -name linux64.h -o -name linux.h -o -name sysv4.h)
do
  cp -uv $file{,.orig}
  sed -e 's@/lib/ld-linux@/lib32/ld-linux@g' $file.orig > $file
  echo '
#undef STANDARD_STARTFILE_PREFIX_1
#define STANDARD_STARTFILE_PREFIX_2 "/lib'$LIB_SUFFIX'/"' >> $file
  touch $file.orig
done