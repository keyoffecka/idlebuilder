if [ "$ARCH" == "32" ] ; then
  make CC="$CC" CXX="$CXX" $COMPILE_OPTS BUILD_OPT=1 NSPR_INCLUDE_DIR=$PREFIX/include/nspr USE_SYSTEM_ZLIB=1 ZLIB_LIBS=-lz NSS_USE_SYSTEM_SQLITE=1
else
  make CC="$CC" CXX="$CXX" $COMPILE_OPTS BUILD_OPT=1 NSPR_INCLUDE_DIR=$PREFIX/include/nspr USE_SYSTEM_ZLIB=1 ZLIB_LIBS=-lz USE_64=1 NSS_USE_SYSTEM_SQLITE=1
fi
